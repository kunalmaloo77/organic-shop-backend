{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" } 
  ],
  "headers": [
    {
      "source": "/(.*)", 
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "https://organic-shop-frontend.vercel.app/" }, 
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, PUT, DELETE, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization, X-Requested-With, Accept, Origin" }
      ]
    }
  ]
}
